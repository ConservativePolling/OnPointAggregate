{
  "version": 2,
  "name": "onpointaggregate",
  "builds": [
    {
      "src": "OnPointAggregate3.0/**",
      "use": "@vercel/static"
    },
    {
      "src": "tracker/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/tracker/(.*)",
      "dest": "/tracker/$1"
    },
    {
      "src": "/tracker",
      "dest": "/tracker"
    },
    {
      "src": "/(.*)",
      "dest": "/OnPointAggregate3.0/$1"
    },
    {
      "src": "/",
      "dest": "/OnPointAggregate3.0/index.html"
    }
  ]
}
